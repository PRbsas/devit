<h1>Communities</h1>

<section id="list">
  <ul>
    <% @communities.each do |community| %>
      <%= link_to community do %>
        <li>
          <%= render partial: 'community', locals: { community: community } %>
        </li>
      <% end %>
    <% end %>
    <%= link_to new_community_path do %>
      <div class="add-link">
        <p ><%= image_tag("plus") %>  Community</p>
      </div>
    <% end %>
  </ul>
</section>
